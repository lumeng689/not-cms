{
  "apps": [
    {
      "name": "not-cms",
      "script": "./dist/backend.js",
      "watch": true,
      "env": {
        "PORT": "8080",
        "NODE_ENV": "production"
      },
      "out_file": "/logs/not-cms/app.log",
      "error_file": "/logs/not-cms/app_error.log",
      "max_memory_restart": "512M",
      "instances": 4,
      "exec_mode": "cluster"
    }
  ]
}
